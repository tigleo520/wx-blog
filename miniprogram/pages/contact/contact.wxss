/* pages/contact/contact.wxss */
.contact-wrap {
  position: relative;
  margin: 100rpx auto;
  padding: 80rpx;
  width: 85vw;
  text-align: center;
  color: #666;
  background-color: rgba(255, 255, 255, 0.7);
  border: 5rpx solid #f1f1f1;
  border-radius: 15rpx;
  box-shadow: 5rpx 5rpx 5rpx #eee;
}
.contact-wrap .name {
  margin-bottom: 20rpx;
  font-size: 60rpx;
  color: #000;
}
.contact-wrap .decs {
  font-size: 28rpx;
}
.contact-wrap .contact {
  margin-top: 250rpx;
  text-align: left;
}
.contact-wrap .contact .ops {
  height: 75rpx;
}
.contact-wrap .contact .iconfont {
  color: #666;
  margin-right: 20rpx;
}
.contact-wrap .icon-ico {
  text-align: right;
  font-size: 50rpx;

}